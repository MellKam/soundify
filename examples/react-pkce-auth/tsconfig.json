{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "rootDir": ".",
    "outDir": "./dist/",
    "target": "ESNext",
    "jsx": "preserve"
  }
}
