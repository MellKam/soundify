{
	"name": "soundify-web-api",
	"type": "module",
	"version": "0.0.15",
	"main": "./dist/mod.js",
	"types": "./dist/mod.d.ts",
	"module": "./dist/mod.js",
	"sideEffects": false,
	"files": [
		"./dist/",
		"./auth/",
		"./api/",
		"./platform/platform.node.ts",
		"./platform/platform.web.ts",
		"mod.ts",
		"client.ts",
		"utils.ts"
	],
	"exports": {
		".": {
			"import": {
				"import": "./dist/mod.js",
				"node": "./dist/mod.js",
				"types": "./dist/mod.d.ts"
			}
		},
		"./web": {
			"import": {
				"import": "./dist/web.mjs",
				"types": "./dist/mod.d.ts"
			}
		}
	},
	"typesVersions": {
		"*": {
			"index": [
				"./dist/mod.d.ts"
			],
			"web": [
				"./dist/mod.d.ts"
			]
		}
	},
	"devDependencies": {
		"typescript": "4.9.5",
		"@types/node": "18.14.6"
	}
}
