{
  "name": "@soundify/web-api",
  "version": "0.2.4",
  "description": "Modern Spotify api wrapper for Node, Deno, and browser ðŸŽ§",
  "type": "module",
  "main": "dist/server.cjs",
  "module": "dist/server.js",
  "types": "types/index.d.ts",
  "browser": {
    "dist/server.cjs": "dist/browser.cjs",
    "dist/server.js": "dist/browser.js"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "types/",
    "src/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "browser": {
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs"
      },
      "node": {
        "import": "./dist/server.js",
        "require": "./dist/server.cjs"
      },
      "deno": "./dist/server.js",
      "bun": "./dist/server.js",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "scripts": {
    "build": "rimraf dist/ types/ && __IS_NODE__=true vite build && __IS_NODE__=false vite build && tsc --emitDeclarationOnly --declaration",
    "types:check": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint:check": "prettier --check ./src/ ./examples",
    "lint": "prettier --write ./src ./examples"
  },
  "devDependencies": {
    "@types/node": "20.9.0",
    "all-contributors-cli": "6.26.1",
    "envalid": "8.0.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vitest": "0.34.6",
    "@vitest/coverage-c8": "0.33.0",
    "vitest-fetch-mock": "0.2.2",
    "@faker-js/faker": "8.2.0"
  },
  "author": {
    "name": "Artem Melnyk",
    "url": "https://github.com/MellKam"
  },
  "packageManager": "pnpm@8.10.3",
  "repository": "github:MellKam/soundify",
  "bugs": {
    "url": "https://github.com/MellKam/soundify/issues"
  },
  "readme": "https://github.com/MellKam/soundify#readme",
  "keywords": [
    "spotify",
    "api",
    "wrapper",
    "music",
    "client",
    "soundify",
    "web",
    "js",
    "ts",
    "deno"
  ]
}
