{
	"name": "soundify-web-api",
	"type": "module",
	"version": "0.0.12",
	"repository": {
		"type": "git",
		"url": "https://github.com/MellKam/soundify"
	},
	"description": "Modern Spotify webapi wrapper for Node, Deno, and browser 🎧",
	"license": "MIT",
	"author": {
		"name": "Artem Melnyk",
		"url": "https://github.com/MellKam"
	},
	"files": [
		"./dist/",
		"./auth/",
		"./api/",
		"./platform/platform.node.ts",
		"./platform/platform.web.ts",
		"mod.ts",
		"client.ts",
		"utils.ts"
	],
	"main": "./dist/mod.js",
	"types": "./dist/mod.d.ts",
	"module": "./dist/mod.js",
	"sideEffects": false,
	"exports": {
		".": {
			"import": {
				"import": "./dist/mod.js",
				"node": "./dist/mod.js",
				"types": "./dist/mod.d.ts"
			}
		},
		"./web": {
			"import": {
				"import": "./dist/web.mjs",
				"types": "./dist/mod.d.ts"
			}
		}
	},
	"typesVersions": {
		"*": {
			"index": [
				"./dist/mod.d.ts"
			],
			"web": [
				"./dist/mod.d.ts"
			]
		}
	},
	"keywords": [
		"spotify",
		"web api",
		"api",
		"soundify",
		"wrapper",
		"client",
		"music"
	],
	"devDependencies": {
		"typescript": "4.9.5",
		"@types/node": "18.14.2"
	},
	"engines": {
		"node": ">=17.5.0"
	},
	"packageManager": "pnpm@7.28.0"
}
